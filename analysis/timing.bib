	keywords = {echo acoustic cues, foraging, Pallas' long-tongued bat, pollinator, spatial memory},
	pages = {913--920},
	file = {ScienceDirect Snapshot:/Users/shambhavi/Zotero/storage/C2EA4WXB/S0003347207004526.html:text/html},
}

