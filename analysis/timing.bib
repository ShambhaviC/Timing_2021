@article{nachev_cognition-mediated_2017,
	title = {Cognition-mediated evolution of low-quality floral nectars},
	volume = {355},
	issn = {0036-8075, 1095-9203},
	url = {https://www.sciencemag.org/lookup/doi/10.1126/science.aah4219},
	doi = {10.1126/science.aah4219},
	abstract = {Plants pollinated by hummingbirds or bats produce dilute nectars even though these animals prefer more concentrated sugar solutions. This mismatch is an unsolved evolutionary paradox. Here we show that lower quality, or more dilute, nectars evolve when the strength of preferring larger quantities or higher qualities of nectar diminishes as magnitudes of the physical stimuli increase. In a virtual evolution experiment conducted in the tropical rainforest, bats visited computer-automated flowers with simulated genomes that evolved relatively dilute nectars. Simulations replicated this evolution only when value functions, which relate the physical stimuli to subjective sensations, were nonlinear. Selection also depended on the supply/demand ratio; bats selected for more dilute nectar when competition for food was higher. We predict such a pattern to generally occur when decision-makers consider multiple value dimensions simultaneously, and increases of psychological value are not fully proportional to increases in physical magnitude.},
	language = {en},
	number = {6320},
	urldate = {2021-05-25},
	journal = {Science},
	author = {Nachev, Vladislav and Stich, Kai Petra and Winter, Clemens and Bond, Alan and Kamil, Alan and Winter, York},
	month = jan,
	year = {2017},
	pages = {75--78},
	file = {Nachev et al. - 2017 - Cognition-mediated evolution of low-quality floral.pdf:/Users/shambhavi/Zotero/storage/STE8HHM3/Nachev et al. - 2017 - Cognition-mediated evolution of low-quality floral.pdf:application/pdf},
}

